<h1>Users</h1>

<ul>
  <% @users.each do |user| %>
    <li>
      <% if user.avatar.attached? %>
        <%= image_tag user.avatar %>
      <% else %>
        <p>No profile picture uploaded.</p>
      <% end %>
      First Name: <%= user.first_name %> <br/>
      Last Name: <%= user.last_name %> <br/>
      Title: <%= user.title %> <br/>
      Bio: <%= user.bio %> <br/>
      Email: <%= user.email %> <br/>
      Gender: <%= user.gender %>
    </li>
  <% end %>
</ul>
